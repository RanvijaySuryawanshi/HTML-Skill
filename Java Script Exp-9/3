#include <stdio.h>

int main() {
    int arr[10], i, temp;
    printf("Enter 10 integers:\n");
    for(i = 0; i < 10; i++) {
        scanf("%d", &arr[i]);
    }

    for(i = 0; i < 10/2; i++) {
        temp = arr[i];
        arr[i] = arr[10 - 1 - i];
        arr[10 - 1 - i] = temp;
    }

    printf("Reversed array:\n");
    for(i = 0; i < 10; i++) {
        printf("%d ", arr[i]);
    }
    return 0;
}
